.app_header
  .app_header__text
    家計簿
  .app_header__link
    = link_to new_account_path do
      今月分へ


.app_contents
  .app_contents__main
    .app_contents__main__title
      過去データ
    .app_contents__main__contents
      - @monthly.each do |monthly|
        - if monthly.created_at != @now
          .app_contents__main__contents__list
            = link_to new_account_path do
              = monthly.created_at.strftime("%Y年%m月")
